    <PropertyList>
      
      <path>wiper-l.ac</path>
      
      
      
      <animation>
	
	<object-name>LHwiper</object-name>
	<object-name>LHwiper.arm</object-name>
	<object-name>LHwiper.blade</object-name>    
      </animation>
 
      <animation>
	<type>rotate</type>
	<object-name>LHwiper</object-name>
	<object-name>LHwiper.blade</object-name>
	<property>controls/wiperl/position-norm</property>
	<interpolation>
	  <entry>
	    <ind>0.0</ind>
	    <dep>-100</dep>
	  </entry>
	  <entry>
	    <ind>1.0</ind>
	    <dep>-25</dep>
	  </entry>
	</interpolation>
	<center>
	  <x-m>-25.290</x-m>
	  <y-m>-0.406</y-m>
	  <z-m>3.359</z-m>
	</center>
	<axis>
	  <x>-0.688</x>
	  <y>-0.235</y>
	  <z>0.687</z>
	</axis>
      </animation>
      
      <animation>
	<type>rotate</type>
	<object-name>LHwiper.arm</object-name>
	<property>controls/wiperl/position-norm</property>
	<interpolation>
	  <entry>
	    <ind>0.0</ind>
	    <dep>-100</dep>
	  </entry>
	  <entry>
	    <ind>1.0</ind>
	    <dep>-25</dep>
	  </entry>
	</interpolation>
	<center>
	  <x-m>-25.325</x-m>
	  <y-m>-0.321</y-m>
	  <z-m>3.390</z-m>
	</center>
	<axis>
	  <x>-0.688</x>
	  <y>-0.235</y>
	  <z>0.687</z>
	</axis>
      </animation>
      
      <animation>
	<type>rotate</type>
	<object-name>LHwiper.blade</object-name>
	<property>controls/wiperl/position-norm</property>
	<interpolation>
	  <entry>
	    <ind>0.0</ind>
	    <dep>45</dep>
	  </entry>
	  <entry>
	    <ind>1.0</ind>
	    <dep>25</dep>
	  </entry>
	</interpolation>
	<center>
	  <x-m>-24.922</x-m>
	  <y-m>-0.059</y-m>
	  <z-m>3.784</z-m>
	</center>
	<axis>
	  <x>-0.688</x>
	  <y>-0.235</y>
	  <z>0.687</z>
	</axis>
      </animation>
      
      </PropertyList>
      
      