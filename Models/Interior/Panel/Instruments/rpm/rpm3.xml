<PropertyList>
 <path>rpm.ac</path>

   <!--Lights-->
 
 <animation>   
   <object-name>group</object-name>
   <object-name>rect1</object-name>
   <object-name>rect2</object-name>   
</animation>

  <animation>
    <type>material</type>
    <object-name>group</object-name>
   <object-name>rect1</object-name>
   <object-name>rect2</object-name> 
    <emission>
      <factor-prop>controls/switches/gauge-light</factor-prop>
      <red>   0.35 </red>
      <green> 0.35 </green>
      <blue>  0.35 </blue>
    </emission>
  </animation>

 
 <animation>
  <type>rotate</type>
  <object-name>rect1</object-name>
  <property>engines/engine[2]/n2</property>
  <interpolation>
   <entry><ind>  0.0</ind><dep>/0</dep></entry> 
   <entry><ind> 30</ind><dep>  90</dep></entry>
   <entry><ind> 60</ind><dep>  180</dep></entry>
   <entry><ind> 90</ind><dep>  270</dep></entry>
   <entry><ind> 110</ind><dep>  340</dep></entry>
  </interpolation>
  <center>
   <x-m>0.0</x-m>
   <y-m>0.0</y-m>
   <z-m>0.0</z-m>
  </center>
  <axis>
   <x>1</x>
   <y>0</y>
   <z>0</z>
  </axis>
 </animation>
 
 <animation>
  <type>rotate</type>
  <object-name>rect2</object-name>
  <property>engines/engine[2]/rpm</property>
  <interpolation>
   <entry><ind>  0.0</ind><dep>/0</dep></entry>   
   <entry><ind> 2502</ind><dep>  310</dep></entry>
  </interpolation>
  <center>
   <x-m>0.0</x-m>
   <y-m>0.0</y-m>
   <z-m>0.0</z-m>
  </center>
  <axis>
   <x>1</x>
   <y>0</y>
   <z>0</z>
  </axis>
 </animation>

</PropertyList>

